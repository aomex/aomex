{
  "name": "@aomex/prisma",
  "version": "3.29.2",
  "description": "从prisma表结构生成对应的验证器",
  "repository": "git@github.com:aomex/aomex.git",
  "homepage": "https://aomex.js.org",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "tsup"
  },
  "types": "src/index.ts",
  "exports": "./src/index.ts",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "bin": {
    "aomex-prisma": "./src/bin.ts"
  },
  "publishConfig": {
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./package.json": "./package.json"
    },
    "access": "public",
    "bin": {
      "aomex-prisma": "./dist/bin.js"
    }
  },
  "peerDependencies": {
    "@aomex/common": "^3.0.0",
    "prisma": ">=6"
  },
  "dependencies": {
    "@prisma/generator-helper": "^7.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@aomex/common": "workspace:^",
    "@prisma/internals": "^7.0.0",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.upperfirst": "^4.3.7",
    "prisma": "^7.0.0"
  }
}
